{
  "name": "cloudflare-pages-example",
  "version": "0.3.18",
  "description": "An example showing Remix-Image on Cloudflare Pages",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "remix setup cloudflare-pages",
    "build": "cross-env NODE_ENV=production remix build",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --binding \"SELF_URL\"=\"http://localhost:8788\"",
    "dev": "cross-env NODE_ENV=development remix build && run-p dev:*",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@remix-run/cloudflare-pages": "^1.2.2",
    "@remix-run/react": "^1.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.2",
    "remix-image": "^0.3.16"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@remix-run/dev": "^1.2.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.5",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
