{
  "private": true,
  "name": "cloudflare-example",
  "description": "An example showing Remix-Image on Cloudflare Workers",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/index.js",
  "scripts": {
    "build": "remix build",
    "deploy": "npm run build && wrangler publish",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "remix build && run-p dev:*",
    "postinstall": "remix setup cloudflare",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@remix-run/cloudflare-workers": "^1.3.3",
    "@remix-run/react": "^1.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.3.3",
    "remix-image": "*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@remix-run/dev": "^1.3.3",
    "@remix-run/eslint-config": "^1.3.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "miniflare": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
